@model FindMyItem.Domain.FeedSearchResult

<table class="table table-striped table-hover">
<caption><h3>Feed Results</h3></caption>
<thead>
    <tr>
        <th></th>
        <th>Title</th>
        <th>Advert URL</th>
    </tr>
</thead>
<tbody>            
        @foreach (var item in Model.FeedResults)
        { 
            <tr>
                <td>@item.ImageURL</td>
                <td>@item.Title</td>
                <td><a href="@item.AdvertURL" title="Go direct to advert @item.AdvertURL" target="_blank">Go to advert</a></td>
            </tr>
        }            
</tbody>
</table>